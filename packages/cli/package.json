    "axios": "^0.23.0",
    "execa": "^5.1.1",
    "koa-static": "^5.0.0",
    "mocha": "^9.1.2",
    "nodemon": "^2.0.13",
    "shx": "^0.3.3",
    "typescript": "^4.4.4",
    "winston": "^3.3.3"
